{
    "_id": "data.hadoop.JobTracking", 
    "averageSeries": false, 
    "columns": 2, 
    "defaultLineWidth": 3, 
    "from": "", 
    "width": 700,
    "height": 300, 
    "timeBack": "48h", 
    "legend": true, 
    "refresh": true, 
    "refreshIntervalSeconds": 120, 
    "theme": "black-tie", 
    "title": "Hadoop Job Analysis Information", 
    "until": "", 
    "user": "hadoop_job_analyzer", 
    "data": [
        {
            "params" : "areaMode=stacked",
            "target": "groupByNode(data.hadoop.jobtracker.myjobtrackerdomain.myjobtrackerhost.${crosssection}.*.JOB_STATUS.SUCCESS.value,6,'sumSeries')",
            "title": "Successful Job Counts per selected cross section"
        },       
        {
            "params" : "areaMode=stacked",
            "target": "groupByNode(data.hadoop.jobtracker.myjobtrackerdomain.myjobtrackerhost.${crosssection}.*.JOB_STATUS.FAILURE.value,6,'sumSeries')",
            "title": "Failed Job Counts per selected cross section"
        }, 
        {
            "params" : "areaMode=stacked",
            "target": "groupByNode(data.hadoop.jobtracker.myjobtrackerdomain.myjobtrackerhost.${crosssection}.*.TOTAL_DURATION.value,6,'sumSeries')",
            "title": "Total job duration per selected cross section"
        },        
        {
            "params" : "areaMode=stacked",
            "target": "groupByNode(data.hadoop.jobtracker.myjobtrackerdomain.myjobtrackerhost.${crosssection}.*.TOTAL_MAPS.value,6,'sumSeries')",
            "title": "Total map tasks per selected cross section"
        },        
        {
            "params" : "areaMode=stacked",
            "target": "groupByNode(data.hadoop.jobtracker.myjobtrackerdomain.myjobtrackerhost.${crosssection}.*.FAILED_MAPS.value,6,'sumSeries')",
            "title": "Total Failed map tasks per "
        },        
	{
            "params" : "areaMode=stacked",
            "target": "groupByNode(data.hadoop.jobtracker.myjobtrackerdomain.myjobtrackerhost.${crosssection}.*.TOTAL_REDUCES.value,6,'sumSeries')",
            "title": "Total reduce tasks per selected cross section"
        },        
        {
            "params" : "areaMode=stacked",
            "target": "groupByNode(data.hadoop.jobtracker.myjobtrackerdomain.myjobtrackerhost.${crosssection}.*.FAILED_REDUCES.value,6,'sumSeries')",
            "title": "Total Failed reduce tasks per selected cross section"
        },        
	{
            "params" : "areaMode=stacked",
            "target": "groupByNode(data.hadoop.jobtracker.myjobtrackerdomain.myjobtrackerhost.${crosssection}.*.COUNTERS.FileSystemCounters.FILE_BYTES_READ.value,6,'sumSeries')",
            "title": "Total file bytes read from disk, per selected cross section"
        },        
        {
            "params" : "areaMode=stacked",
            "target": "groupByNode(data.hadoop.jobtracker.myjobtrackerdomain.myjobtrackerhost.${crosssection}.*.COUNTERS.FileSystemCounters.FILE_BYTES_WRITTEN.value,6,'sumSeries')",
            "title": "Total file bytes written to disk, per selected cross section"
        },
	{
            "params" : "areaMode=stacked",
            "target": "groupByNode(data.hadoop.jobtracker.myjobtrackerdomain.myjobtrackerhost.${crosssection}.*.COUNTERS.org_apache_hadoop_mapred_Task__Counter.MAP_INPUT_RECORDS.value,6,'sumSeries')",
            "title": "Map Tasks total input records, per selected cross section"
        },
	{
            "params" : "areaMode=stacked",
            "target": "groupByNode(data.hadoop.jobtracker.myjobtrackerdomain.myjobtrackerhost.${crosssection}.*.COUNTERS.org_apache_hadoop_mapred_Task__Counter.MAP_OUTPUT_RECORDS.value,6,'sumSeries')",
            "title": "Map Tasks total output records, per selected cross section"
        },
	{
            "params" : "areaMode=stacked",
            "target": "groupByNode(data.hadoop.jobtracker.myjobtrackerdomain.myjobtrackerhost.${crosssection}.*.COUNTERS.org_apache_hadoop_mapred_Task__Counter.REDUCE_INPUT_RECORDS.value,6,'sumSeries')",
            "title": "Reduce Tasks total input records, per selected cross section"
        },
	{
            "params" : "areaMode=stacked",
            "target": "groupByNode(data.hadoop.jobtracker.myjobtrackerdomain.myjobtrackerhost.${crosssection}.*.COUNTERS.org_apache_hadoop_mapred_Task__Counter.REDUCE_OUTPUT_RECORDS.value,6,'sumSeries')",
            "title": "Reduce Tasks total output records, per selected cross section"
        },
	{
            "params" : "areaMode=stacked",
            "target": "groupByNode(data.hadoop.jobtracker.myjobtrackerdomain.myjobtrackerhost.${crosssection}.*.COUNTERS.org_apache_hadoop_mapred_Task__Counter.REDUCE_SHUFFLE_BYTES.value,6,'sumSeries')",
            "title": "Reduce Shuffle Bytes, per selected cross section"
        },
	{
            "params" : "areaMode=stacked",
            "target": "groupByNode(data.hadoop.jobtracker.myjobtrackerdomain.myjobtrackerhost.${crosssection}.*.COUNTERS.org_apache_hadoop_mapred_Task__Counter.SPILLED_RECORDS.value,6,'sumSeries')",
            "title": "Spilled record count, per selected cross section"
        }
    ], 
    "parameters": {
        "crosssection": {
            "type": "dynamic",
            "query": "data.hadoop.jobtracker.myjobtrackerdomain.myjobtrackerhost.*",
            "index": 5,
            "showAll": false
        }
    }
}
